{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/bin": true,
        "**/obj": true,
        "**/*.meta": true,
        "**/*.fbx": true,
        "Cache/": true,
        "Libraries/": true
    },
    "workbench.colorTheme": "Default Dark Modern",
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "*.deprecated": {
                "fontStyle": "strikethrough"
            }
        }
    },
    "path-intellisense.extensionOnImport": true
}
